<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="366.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fenix.timemanagementfrontend.controller.create.CreateCategoryController">
    <TextField fx:id="name" layoutX="219.0" layoutY="148.0" promptText="Name" />
    <ComboBox fx:id="interval" layoutX="219.0" layoutY="190.0" prefHeight="28.0" prefWidth="162.0" />
    <TextField fx:id="value" layoutX="219.0" layoutY="233.0" promptText="Min Hours Per Interval" />
    <Button layoutX="324.0" layoutY="275.0" mnemonicParsing="false" onAction="#create" text="Create" />
    <Button layoutX="219.0" layoutY="275.0" mnemonicParsing="false" onAction="#back" text="Cancel" />
    <Label layoutX="201.0" layoutY="97.0" text="Create Category">
        <font>
            <Font size="26.0" />
        </font>
    </Label>
    <Label fx:id="error" layoutX="249.0" layoutY="243.0" text="Pleas fill all fields" textFill="RED" visible="false" />
</AnchorPane>
